@article{strasserLinguaRealisLingua1989,
  title = {{Lingua realis, lingua universalis und lingua cryptologica: Analogiebildungen bei den Universalsprachen des 16. und 17. Jahrhunderts}},
  shorttitle = {{Lingua realis, lingua universalis und lingua cryptologica}},
  author = {Strasser, Gerhard F.},
