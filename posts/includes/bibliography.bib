@article{wrightPussyAinWorth2020,
  title = {`{{The}} Pussy Ain't Worth It, Bro': Assessing the Discourse and Structure of {{MGTOW}}},
  shorttitle = {`{{The}} Pussy Ain't Worth It, Bro'},
  author = {Wright, Scott and Trott, Verity and Jones, Callum},
