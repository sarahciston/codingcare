@article{williamsHowAlgorithmsDiscriminate2018,
  title = {How {{Algorithms Discriminate Based}} on {{Data}} They {{Lack}}: {{Challenges}}, {{Solutions}}, and {{Policy Implications}}},
  shorttitle = {How {{Algorithms Discriminate Based}} on {{Data}} They {{Lack}}},
  author = {Williams, Betsy Anne and Brooks, Catherine F. and Shmargad, Yotam},
  year = {2018},
  journal = {Journal of Information Policy},
