@incollection{oakesAuthorProfilingRelated2022,
  title = {Author {{Profiling}} and {{Related Applications}}},
  booktitle = {The {{Oxford Handbook}} of {{Computational Linguistics}}},
  author = {Oakes, Michael P.},
