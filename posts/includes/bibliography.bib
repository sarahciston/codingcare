@misc{darAnalyzingTransformersEmbedding2022,
  title = {Analyzing {{Transformers}} in {{Embedding Space}}},
  author = {Dar, Guy and Geva, Mor and Gupta, Ankit and Berant, Jonathan},
  year = {2022},
