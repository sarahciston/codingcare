@article{clark-parsonsSEEYOUBELIEVE2019,
  title = {``{{I SEE YOU}}, {{I BELIEVE YOU}}, {{I STAND WITH YOU}}'': \#{{MeToo}} and the Performance of Networked Feminist Visibility},
  shorttitle = {``{{I SEE YOU}}, {{I BELIEVE YOU}}, {{I STAND WITH YOU}}''},
